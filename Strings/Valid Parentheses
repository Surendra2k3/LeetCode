import java.util.Scanner;

public class Main{
    public static boolean isValidParanthesis(String s){
    while(true){
        if(s.contains("()")){
            s=s.replace("()","");
        }
        else if(s.contains("[]")){
            s=s.replace("[]", "");
        }
        else if(s.contains("{}")){
            s=s.replace("{}", "");
        }
        else{
            return s.isEmpty();
        }
    }
}

public static void main(String[] args) {
    Scanner sc=new Scanner(System.in);
    System.out.print("Enter the Parathesis Format: ");
    String s1=sc.nextLine();
    boolean res=isValidParanthesis(s1);
    if(res==true){
        System.out.println("It is a Valid Paranthesis");
    }
    else{
        System.out.println("It is an Invalid Paranthesis");
    }
    sc.close();
}
}
